        </dependency>
        <dependency>
            <groupId>com.lianjia.ehr.personnel</groupId>
            <artifactId>api</artifactId>
        </dependency>
    </dependencies>


    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.5.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <encoding>UTF-8</encoding>
                    <fork>true</fork>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <distributionManagement>
        <repository>
            <id>releases</id>
            <name>Releases</name>
            <url>http://maven.lianjia.com/content/repositories/Lianjia-Home-Releases/</url>
        </repository>
        <snapshotRepository>
            <id>releases</id>
            <name>Snapshot</name>
            <url>http://maven.lianjia.com/content/repositories/Lianjia-Home-Snapshots/</url>
        </snapshotRepository>
    </distributionManagement>
</project>
