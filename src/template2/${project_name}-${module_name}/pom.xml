    <modelVersion>4.0.0</modelVersion>

    <groupId>com.lianjia.${project_name}.${module_name}</groupId>
    <artifactId>${project_name}-${module_name}-parent</artifactId>
    <packaging>pom</packaging>
    <version>1.0.0-SNAPSHOT</version>

    <modules>
